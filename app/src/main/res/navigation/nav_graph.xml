<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment"> <fragment
    android:id="@+id/splashFragment"
    android:name="com.project.unimate.ui.splash.SplashFragment"
    tools:layout="@layout/fragment_splash">
    <action
        android:id="@+id/action_splash_to_login"
        app:destination="@id/loginFragment"
        app:popUpTo="@id/splashFragment"
        app:popUpToInclusive="true" />
</fragment>

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.project.unimate.ui.login.LoginFragment"
        tools:layout="@layout/activity_login">
        <action
            android:id="@+id/action_login_to_profileCreate"
            app:destination="@id/profileCreateFragment" />
    </fragment>

    <fragment
        android:id="@+id/profileCreateFragment"
        android:name="com.project.unimate.ui.profile.ProfileCreateFragment"
        tools:layout="@layout/activity_profile_create">
        <action
            android:id="@+id/action_profileCreate_to_team_nav"
            app:destination="@id/team_nav"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>

    <include app:graph="@navigation/team_nav" />

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.project.unimate.ui.home.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/calendarFragment"
        android:name="com.project.unimate.ui.calendar.CalendarFragment"
        tools:layout="@layout/fragment_calendar" />

    <fragment
        android:id="@+id/cockFragment"
        android:name="com.project.unimate.ui.cock.CockFragment"
        tools:layout="@layout/fragment_cock" />

    <fragment
        android:id="@+id/myPageFragment"
        android:name="com.project.unimate.ui.mypage.MyPageFragment"
        tools:layout="@layout/fragment_my_page" />



</navigation>